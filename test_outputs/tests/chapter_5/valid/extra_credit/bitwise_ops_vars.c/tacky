Program {
    static_variables: [],
    functions: [
        Function {
            identifier: "main",
            global: true,
            params: [],
            instructions: [
                Copy {
                    src: 3,
                    dst: Var("a.1", Int),
                },
                Copy {
                    src: 5,
                    dst: Var("b.2", Int),
                },
                Copy {
                    src: 8,
                    dst: Var("c.3", Int),
                },
                Binary {
                    op: BitwiseAnd,
                    lhs: Var("a.1", Int),
                    rhs: Var("b.2", Int),
                    dst: Var("main.tmp.0", Int),
                },
                Binary {
                    op: BitwiseOr,
                    lhs: Var("main.tmp.0", Int),
                    rhs: Var("c.3", Int),
                    dst: Var("main.tmp.1", Int),
                },
                Return(
                    Var("main.tmp.1", Int),
                ),
                Return(
                    0,
                ),
            ],
        },
    ],
}