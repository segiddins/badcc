Program {
    static_variables: [
        StaticVariable {
            identifier: "a",
            global: true,
            init: 5,
            width: Four,
        },
    ],
    functions: [
        Function {
            identifier: "return_a",
            global: true,
            params: [],
            instructions: [
                Return(
                    Var(
                        "a",
                        Int,
                    ),
                ),
                Return(
                    Constant(
                        Int(
                            0,
                        ),
                    ),
                ),
            ],
        },
        Function {
            identifier: "main",
            global: true,
            params: [],
            instructions: [
                Copy {
                    src: Constant(
                        Int(
                            3,
                        ),
                    ),
                    dst: Var(
                        "a.3",
                        Int,
                    ),
                },
                Binary {
                    op: NotEqual,
                    lhs: Var(
                        "a",
                        Int,
                    ),
                    rhs: Constant(
                        Int(
                            5,
                        ),
                    ),
                    dst: Var(
                        "main.tmp.0",
                        Int,
                    ),
                },
                JumpIfZero(
                    Var(
                        "main.tmp.0",
                        Int,
                    ),
                    "main.0.true",
                ),
                Return(
                    Constant(
                        Int(
                            1,
                        ),
                    ),
                ),
                Jump(
                    "main.0.end",
                ),
                Label(
                    "main.0.true",
                ),
                Label(
                    "main.0.end",
                ),
                Copy {
                    src: Constant(
                        Int(
                            4,
                        ),
                    ),
                    dst: Var(
                        "a",
                        Int,
                    ),
                },
                Call(
                    "return_a",
                    [],
                    Var(
                        "main.tmp.1",
                        Int,
                    ),
                ),
                Binary {
                    op: Add,
                    lhs: Var(
                        "a.3",
                        Int,
                    ),
                    rhs: Var(
                        "main.tmp.1",
                        Int,
                    ),
                    dst: Var(
                        "main.tmp.2",
                        Int,
                    ),
                },
                Return(
                    Var(
                        "main.tmp.2",
                        Int,
                    ),
                ),
                Return(
                    Constant(
                        Int(
                            0,
                        ),
                    ),
                ),
            ],
        },
    ],
}