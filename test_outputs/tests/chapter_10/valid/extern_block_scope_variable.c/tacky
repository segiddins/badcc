Program {
    static_variables: [
        StaticVariable {
            identifier: "foo",
            global: true,
            init: 3,
            width: Four,
        },
    ],
    functions: [
        Function {
            identifier: "main",
            global: true,
            params: [],
            instructions: [
                Copy {
                    src: Constant(
                        Int(
                            1,
                        ),
                    ),
                    dst: Var(
                        "outer.1",
                        Int,
                    ),
                },
                Copy {
                    src: Constant(
                        Int(
                            0,
                        ),
                    ),
                    dst: Var(
                        "foo.2",
                        Int,
                    ),
                },
                JumpIfZero(
                    Var(
                        "outer.1",
                        Int,
                    ),
                    "main.0.true",
                ),
                Return(
                    Var(
                        "foo",
                        Int,
                    ),
                ),
                Jump(
                    "main.0.end",
                ),
                Label(
                    "main.0.true",
                ),
                Label(
                    "main.0.end",
                ),
                Return(
                    Constant(
                        Int(
                            0,
                        ),
                    ),
                ),
                Return(
                    Constant(
                        Int(
                            0,
                        ),
                    ),
                ),
            ],
        },
    ],
}