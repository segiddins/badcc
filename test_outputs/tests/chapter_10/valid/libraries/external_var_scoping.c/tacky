Program {
    static_variables: [],
    functions: [
        Function {
            identifier: "read_x",
            global: true,
            params: [],
            instructions: [
                Copy {
                    src: 4,
                    dst: Var("x.1", Int),
                },
                Binary {
                    op: Equals,
                    lhs: Var("x.1", Int),
                    rhs: 4,
                    dst: Var("read_x.tmp.0", Int),
                },
                JumpIfZero(
                    Var("read_x.tmp.0", Int),
                    "read_x.0.true",
                ),
                Return(
                    Var("x", Int),
                ),
                Jump(
                    "read_x.0.end",
                ),
                Label(
                    "read_x.0.true",
                ),
                Unary {
                    op: Negate,
                    src: 1,
                    dst: Var("read_x.tmp.1", Int),
                },
                Return(
                    Var("read_x.tmp.1", Int),
                ),
                Label(
                    "read_x.0.end",
                ),
                Return(
                    0,
                ),
            ],
        },
    ],
}