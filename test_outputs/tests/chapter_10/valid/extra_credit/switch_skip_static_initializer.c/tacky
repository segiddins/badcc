Program {
    static_variables: [
        StaticVariable {
            identifier: "a",
            global: true,
            init: 3,
            width: Four,
        },
        StaticVariable {
            identifier: "x.2",
            global: false,
            init: 10,
            width: Four,
        },
    ],
    functions: [
        Function {
            identifier: "main",
            global: true,
            params: [],
            instructions: [
                Jump(
                    "switch.0.cases",
                ),
                Label(
                    "switch.0.1",
                ),
                Copy {
                    src: Constant(
                        Int(
                            0,
                        ),
                    ),
                    dst: Var(
                        "x.2",
                        Int,
                    ),
                },
                Label(
                    "switch.0.3",
                ),
                Return(
                    Var(
                        "x.2",
                        Int,
                    ),
                ),
                Jump(
                    "switch.0",
                ),
                Label(
                    "switch.0.cases",
                ),
                Binary {
                    op: Equals,
                    lhs: Constant(
                        Int(
                            1,
                        ),
                    ),
                    rhs: Var(
                        "a",
                        Int,
                    ),
                    dst: Var(
                        "main.tmp.0",
                        Int,
                    ),
                },
                JumpIfNotZero(
                    Var(
                        "main.tmp.0",
                        Int,
                    ),
                    "switch.0.1",
                ),
                Binary {
                    op: Equals,
                    lhs: Constant(
                        Int(
                            3,
                        ),
                    ),
                    rhs: Var(
                        "a",
                        Int,
                    ),
                    dst: Var(
                        "main.tmp.0",
                        Int,
                    ),
                },
                JumpIfNotZero(
                    Var(
                        "main.tmp.0",
                        Int,
                    ),
                    "switch.0.3",
                ),
                Label(
                    "switch.0",
                ),
                Return(
                    Constant(
                        Int(
                            0,
                        ),
                    ),
                ),
                Return(
                    Constant(
                        Int(
                            0,
                        ),
                    ),
                ),
            ],
        },
    ],
}