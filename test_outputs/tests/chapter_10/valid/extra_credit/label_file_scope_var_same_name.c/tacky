    static_variables: [
        StaticVariable {
            identifier: "x",
            global: true,
            init: 0,
            width: Four,
        },
    ],
    functions: [
        Function {
            identifier: "main",
            global: true,
            params: [],
            instructions: [
                Copy {
                    src: 10,
                    dst: Var("x.2", Int),
                },
                Jump(
                    "main.x",
                ),
                Return(
                    Var("x.2", Int),
                ),
                Label(
                    "main.x",
                ),
                Return(
                    Var("x", Int),
                ),
