Program {
    static_variables: [],
    functions: [
        Function {
            identifier: "main",
            global: true,
            params: [],
            instructions: [
                Copy {
                    src: 3l,
                    dst: Var("x.5", Long),
                },
                Copy {
                    src: 4l,
                    dst: Var("y.6", Long),
                },
                Copy {
                    src: 5,
                    dst: Var("z.7", Int),
                },
                Call(
                    "add_variables",
                    [
                        Var("x.5", Long),
                        Var("y.6", Long),
                        Var("z.7", Int),
                    ],
                    Var("main.tmp.0", Long),
                ),
                Truncate {
                    src: Var("main.tmp.0", Long),
                    dst: Var("main.tmp.1", Int),
                },
                Return(
                    Var("main.tmp.1", Int),
                ),
                Return(
                    0,
                ),
            ],
        },
    ],
}