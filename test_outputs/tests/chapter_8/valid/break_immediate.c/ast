    function: Function {
        name: "main",
        body: Block {
            items: [
                Declaration(
                    VariableDeclaration {
                        name: "a",
                        init: Some(
                            Constant(
                                10,
                            ),
                        ),
                    },
                ),
                Statement(
                    While(
                        Assignment(
                            Var(
                                "a",
                            ),
                            Constant(
                                1,
                            ),
                        ),
                        Break(
                            None,
                        ),
                        None,
                    ),
                ),
                Statement(
                    Return(
                        Var(
                            "a",
                        ),
                    ),
                ),
            ],
        },
    },
}