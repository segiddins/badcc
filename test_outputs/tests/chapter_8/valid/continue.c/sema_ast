Program {
    declarations: [
        FunctionDeclaration {
            identifier: "main",
            params: [],
            ret: Int,
            body: [
                VariableDeclaration {
                    name: "sum.1",
                    ty: Int,
                    init: Constant(0),
                },
                VariableDeclaration {
                    name: "counter.2",
                    ty: Int,
                },
                For {
                    init: VariableDeclaration {
                        name: "i.3",
                        ty: Int,
                        init: Constant(0),
                    },
                    condition: Binary {
                        op: LessThanOrEqual,
                        lhs: Var("i.3"),
                        rhs: Constant(10),
                    },
                    post: Assignment {
                        lhs: Var("i.3"),
                        rhs: Binary {
                            op: Add,
                            lhs: Var("i.3"),
                            rhs: Constant(1),
                        },
                    },
                    body: Compound(
                        [
                            Expression(
                                Assignment {
                                    lhs: Var("counter.2"),
                                    rhs: Var("i.3"),
                                },
                            ),
                            If(
                                Binary {
                                    op: Equals,
                                    lhs: Binary {
                                        op: Remainder,
                                        lhs: Var("i.3"),
                                        rhs: Constant(2),
                                    },
                                    rhs: Constant(0),
                                },
                                Continue {
                                    label: "loop.0",
                                },
                                None,
                            ),
                            Expression(
                                Assignment {
                                    lhs: Var("sum.1"),
                                    rhs: Binary {
                                        op: Add,
                                        lhs: Var("sum.1"),
                                        rhs: Constant(1),
                                    },
                                },
                            ),
                        ],
                    ),
                    label: "loop.0",
                },
                Return(
                    Binary {
                        op: And,
                        lhs: Binary {
                            op: Equals,
                            lhs: Var("sum.1"),
                            rhs: Constant(5),
                        },
                        rhs: Binary {
                            op: Equals,
                            lhs: Var("counter.2"),
                            rhs: Constant(10),
                        },
                    },
                ),
            ],
        },
    ],
}