Program {
    declarations: [
        FunctionDeclaration {
            identifier: "main",
            params: [],
            ret: Int,
            body: [
                VariableDeclaration {
                    name: "sum",
                    ty: Int,
                    init: Constant(0),
                },
                For {
                    init: VariableDeclaration {
                        name: "i",
                        ty: Int,
                        init: Constant(0),
                    },
                    condition: Binary {
                        op: LessThan,
                        lhs: Var("i"),
                        rhs: Constant(10),
                    },
                    post: Assignment {
                        lhs: Var("i"),
                        rhs: Binary {
                            op: Add,
                            lhs: Var("i"),
                            rhs: Constant(1),
                        },
                    },
                    body: Compound(
                        [
                            Switch(
                                Binary {
                                    op: Remainder,
                                    lhs: Var("i"),
                                    rhs: Constant(2),
                                },
                                Compound(
                                    [
                                        Case {
                                            expression: Constant(0),
                                            statement: Continue,
                                        },
                                        Default {
                                            statement: Expression(
                                                Assignment {
                                                    lhs: Var("sum"),
                                                    rhs: Binary {
                                                        op: Add,
                                                        lhs: Var("sum"),
                                                        rhs: Constant(1),
                                                    },
                                                },
                                            ),
                                        },
                                    ],
                                ),
                                None,
                            ),
                        ],
                    ),
                },
                Return(
                    Var("sum"),
                ),
            ],
        },
    ],
}