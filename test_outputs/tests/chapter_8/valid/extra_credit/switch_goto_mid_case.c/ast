                    Constant(4),
                    Compound(
                        [
                            Case {
                                expression: Constant(4),
                                statement: Expression(
                                    Assignment {
                                        lhs: Var("a"),
                                        rhs: Constant(5),
                                    },
                                ),
                            },
                            Labeled(
                                "mid_case",
                                Expression(
                                    Assignment {
                                        lhs: Var("a"),
