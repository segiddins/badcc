Program {
    declarations: [
        FunctionDeclaration {
            identifier: "main",
            params: [],
            ret: Int,
            body: [
                VariableDeclaration {
                    name: "i",
                    ty: Int,
                    init: Constant(1),
                },
                DoWhile(
                    Compound(
                        [
                            Labeled(
                                "while_start",
                                Expression(
                                    Assignment {
                                        lhs: Var("i"),
                                        rhs: Binary {
                                            op: Add,
                                            lhs: Var("i"),
                                            rhs: Constant(1),
                                        },
                                    },
                                ),
                            ),
                            If(
                                Binary {
                                    op: LessThan,
                                    lhs: Var("i"),
                                    rhs: Constant(10),
                                },
                                Goto("while_start"),
                                None,
                            ),
                        ],
                    ),
                    Constant(0),
                    None,
                ),
