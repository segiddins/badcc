Program {
    declarations: [
        FunctionDeclaration {
            identifier: "main",
            params: [],
            ret: Int,
            body: [
                VariableDeclaration {
                    name: "sum",
                    ty: Int,
                    init: Constant(0),
                },
                For {
                    init: VariableDeclaration {
                        name: "i",
                        ty: Int,
                        init: Constant(0),
                    },
                    post: Assignment {
                        lhs: Var("i"),
                        rhs: Constant(0),
                    },
                    body: Compound(
                        [
                            Labeled(
                                "lbl",
                                Expression(
                                    Assignment {
                                        lhs: Var("sum"),
                                        rhs: Binary {
                                            op: Add,
                                            lhs: Var("sum"),
                                            rhs: Constant(1),
                                        },
                                    },
                                ),
                            ),
                            Expression(
                                Assignment {
                                    lhs: Var("i"),
                                    rhs: Binary {
                                        op: Add,
                                        lhs: Var("i"),
                                        rhs: Constant(1),
                                    },
                                },
                            ),
                            If(
                                Binary {
                                    op: GreaterThan,
                                    lhs: Var("i"),
                                    rhs: Constant(10),
                                },
                                Break,
                                None,
                            ),
                            Goto("lbl"),
                        ],
                    ),
                },
                Return(
                    Var("sum"),
                ),
            ],
        },
    ],
}