Program {
    static_variables: [],
    functions: [
        Function {
            identifier: "main",
            global: true,
            params: [],
            instructions: [
                Copy {
                    src: 100,
                    dst: Var("i.1", Int),
                },
                Copy {
                    src: 0,
                    dst: Var("sum.2", Int),
                },
                Label(
                    "loop.0.head",
                ),
                Binary {
                    op: Add,
                    lhs: Var("sum.2", Int),
                    rhs: 2,
                    dst: Var("main.tmp.0", Int),
                },
                Copy {
                    src: Var("main.tmp.0", Int),
                    dst: Var("sum.2", Int),
                },
                Label(
                    "loop.0.start",
                ),
                Binary {
                    op: Subtract,
                    lhs: Var("i.1", Int),
                    rhs: 1,
                    dst: Var("main.tmp.1", Int),
                },
                Copy {
                    src: Var("main.tmp.1", Int),
                    dst: Var("i.1", Int),
                },
                JumpIfNotZero(
                    Var("i.1", Int),
                    "loop.0.head",
                ),
                Label(
                    "loop.0",
                ),
                Binary {
                    op: Equals,
                    lhs: Var("i.1", Int),
                    rhs: 0,
                    dst: Var("main.tmp.3", Int),
                },
                JumpIfZero(
                    Var("main.tmp.3", Int),
                    "main.0.false",
                ),
                Binary {
                    op: Equals,
                    lhs: Var("sum.2", Int),
                    rhs: 200,
                    dst: Var("main.tmp.4", Int),
                },
                JumpIfZero(
                    Var("main.tmp.4", Int),
                    "main.0.false",
                ),
                Copy {
                    src: 1,
                    dst: Var("main.tmp.2", Int),
                },
                Jump(
                    "main.0.end",
                ),
                Label(
                    "main.0.false",
                ),
                Copy {
                    src: 0,
                    dst: Var("main.tmp.2", Int),
                },
                Label(
                    "main.0.end",
                ),
                Return(
                    Var("main.tmp.2", Int),
                ),
                Return(
                    0,
                ),
            ],
        },
    ],
}