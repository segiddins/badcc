Program {
    static_variables: [],
    functions: [
        Function {
            identifier: "main",
            global: true,
            params: [],
            instructions: [
                Copy {
                    src: 1,
                    dst: Var("flag.1", Int),
                },
                Copy {
                    src: 0,
                    dst: Var("a.2", Int),
                },
                JumpIfZero(
                    Var("flag.1", Int),
                    "main.0.true",
                ),
                Copy {
                    src: 1,
                    dst: Var("a.2", Int),
                },
                Copy {
                    src: Var("a.2", Int),
                    dst: Var("main.tmp.0", Int),
                },
                Jump(
                    "main.0.end",
                ),
                Label(
                    "main.0.true",
                ),
                Copy {
                    src: 0,
                    dst: Var("a.2", Int),
                },
                Copy {
                    src: Var("a.2", Int),
                    dst: Var("main.tmp.0", Int),
                },
                Label(
                    "main.0.end",
                ),
                Return(
                    Var("a.2", Int),
                ),
                Return(
                    0,
                ),
            ],
        },
    ],
}