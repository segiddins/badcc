Program {
    static_variables: [],
    functions: [
        Function {
            identifier: "main",
            global: true,
            params: [],
            instructions: [
                Copy {
                    src: 0,
                    dst: Var("a.1", Int),
                },
                Copy {
                    src: 0,
                    dst: Var("b.2", Int),
                },
                JumpIfZero(
                    Var("a.1", Int),
                    "main.0.true",
                ),
                Copy {
                    src: 2,
                    dst: Var("a.1", Int),
                },
                Jump(
                    "main.0.end",
                ),
                Label(
                    "main.0.true",
                ),
                Copy {
                    src: 3,
                    dst: Var("a.1", Int),
                },
                Label(
                    "main.0.end",
                ),
                JumpIfZero(
                    Var("b.2", Int),
                    "main.1.true",
                ),
                Copy {
                    src: 4,
                    dst: Var("b.2", Int),
                },
                Jump(
                    "main.1.end",
                ),
                Label(
                    "main.1.true",
                ),
                Copy {
                    src: 5,
                    dst: Var("b.2", Int),
                },
                Label(
                    "main.1.end",
                ),
                Binary {
                    op: Add,
                    lhs: Var("a.1", Int),
                    rhs: Var("b.2", Int),
                    dst: Var("main.tmp.0", Int),
                },
                Return(
                    Var("main.tmp.0", Int),
                ),
                Return(
                    0,
                ),
            ],
        },
    ],
}