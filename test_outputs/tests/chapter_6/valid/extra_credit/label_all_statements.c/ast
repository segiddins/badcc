Program {
    declarations: [
        FunctionDeclaration {
            identifier: "main",
            params: [],
            ret: Int,
            body: [
                VariableDeclaration {
                    name: "a",
                    ty: Int,
                    init: Constant {
                        constant: 1,
                    },
                },
                Labeled(
                    "label_if",
                    If(
                        Var {
                            name: "a",
                        },
                        Goto(
                            "label_expression",
                        ),
                        Some(
                            Goto(
                                "label_empty",
                            ),
                        ),
                    ),
                ),
                Labeled(
                    "label_goto",
                    Goto(
                        "label_return",
                    ),
                ),
                If(
                    Constant {
                        constant: 0,
                    },
                    Labeled(
                        "label_expression",
                        Expression(
                            Assignment {
                                lhs: Var {
                                    name: "a",
                                },
                                rhs: Constant {
                                    constant: 0,
                                },
                            },
                        ),
                    ),
                    None,
                ),
                Goto(
                    "label_if",
                ),
                Labeled(
                    "label_return",
                    Return(
                        Var {
                            name: "a",
                        },
                    ),
                ),
                Labeled(
                    "label_empty",
                    Null,
                ),
                Expression(
                    Assignment {
                        lhs: Var {
                            name: "a",
                        },
                        rhs: Constant {
                            constant: 100,
                        },
                    },
                ),
                Goto(
                    "label_goto",
                ),
            ],
        },
    ],
}