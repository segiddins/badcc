Program {
    function: Function {
        name: "main",
        body: Block {
            items: [
                Declaration(
                    VariableDeclaration {
                        name: "a",
                        init: Some(
                            Constant(
                                1,
                            ),
                        ),
                    },
                ),
                Statement(
                    Labeled(
                        "label_if",
                        If(
                            Var(
                                "a",
                            ),
                            Goto(
                                "label_expression",
                            ),
                            Some(
                                Goto(
                                    "label_empty",
                                ),
                            ),
                        ),
                    ),
                ),
                Statement(
                    Labeled(
                        "label_goto",
                        Goto(
                            "label_return",
                        ),
                    ),
                ),
                Statement(
                    If(
                        Constant(
                            0,
                        ),
                        Labeled(
                            "label_expression",
                            Expression(
                                Assignment(
                                    Var(
                                        "a",
                                    ),
                                    Constant(
                                        0,
                                    ),
                                ),
                            ),
                        ),
                        None,
                    ),
                ),
                Statement(
                    Goto(
                        "label_if",
                    ),
                ),
                Statement(
                    Labeled(
                        "label_return",
                        Return(
                            Var(
                                "a",
                            ),
                        ),
                    ),
                ),
                Statement(
                    Labeled(
                        "label_empty",
                        Null,
                    ),
                ),
                Statement(
                    Expression(
                        Assignment(
                            Var(
                                "a",
                            ),
                            Constant(
                                100,
                            ),
                        ),
                    ),
                ),
                Statement(
                    Goto(
                        "label_goto",
                    ),
                ),
            ],
        },
    },
}