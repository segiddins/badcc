Program {
    declarations: [
        FunctionDeclaration {
            identifier: "main",
            params: [],
            ret: Int,
            body: [
                VariableDeclaration {
                    name: "a.1",
                    ty: Int,
                    init: Constant(1),
                },
                Labeled(
                    "label_if",
                    If(
                        Var("a.1"),
                        Goto("label_expression"),
                        Some(
                            Goto("label_empty"),
                        ),
                    ),
                ),
                Labeled(
                    "label_goto",
                    Goto("label_return"),
                ),
                If(
                    Constant(0),
                    Labeled(
                        "label_expression",
                        Expression(
                            Assignment {
                                lhs: Var("a.1"),
                                rhs: Constant(0),
                            },
                        ),
                    ),
                    None,
                ),
                Goto("label_if"),
                Labeled(
                    "label_return",
                    Return(
                        Var("a.1"),
                    ),
                ),
                Labeled(
                    "label_empty",
                    Null,
                ),
                Expression(
                    Assignment {
                        lhs: Var("a.1"),
                        rhs: Constant(100),
                    },
                ),
                Goto("label_goto"),
            ],
        },
    ],
}