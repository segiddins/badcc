Program {
    static_variables: [],
    functions: [
        Function {
            identifier: "main",
            global: true,
            params: [],
            instructions: [
                Copy {
                    src: 10,
                    dst: Var("x.1", Int),
                },
                Binary {
                    op: Subtract,
                    lhs: Var("x.1", Int),
                    rhs: 10,
                    dst: Var("main.tmp.0", Int),
                },
                JumpIfZero(
                    Var("main.tmp.0", Int),
                    "main.0.true",
                ),
                Copy {
                    src: 0,
                    dst: Var("main.tmp.1", Int),
                },
                Jump(
                    "main.0.end",
                ),
                Label(
                    "main.0.true",
                ),
                Copy {
                    src: Var("x.1", Int),
                    dst: Var("main.tmp.2", Int),
                },
                Binary {
                    op: Add,
                    lhs: Var("x.1", Int),
                    rhs: -1,
                    dst: Var("x.1", Int),
                },
                Copy {
                    src: Var("main.tmp.2", Int),
                    dst: Var("main.tmp.1", Int),
                },
                Label(
                    "main.0.end",
                ),
                Return(
                    Var("x.1", Int),
                ),
                Return(
                    0,
                ),
            ],
        },
    ],
}