Program {
    function: Function {
        name: "main",
        body: Block {
            items: [
                Declaration(
                    VariableDeclaration {
                        name: "x",
                        init: Some(
                            Constant(
                                0,
                            ),
                        ),
                    },
                ),
                Statement(
                    If(
                        Binary(
                            NotEqual,
                            Var(
                                "x",
                            ),
                            Constant(
                                0,
                            ),
                        ),
                        Compound(
                            Block {
                                items: [
                                    Statement(
                                        Labeled(
                                            "return_y",
                                            Return(
                                                Var(
                                                    "y",
                                                ),
                                            ),
                                        ),
                                    ),
                                ],
                            },
                        ),
                        None,
                    ),
                ),
                Declaration(
                    VariableDeclaration {
                        name: "y",
                        init: Some(
                            Constant(
                                4,
                            ),
                        ),
                    },
                ),
                Statement(
                    Goto(
                        "return_y",
                    ),
                ),
            ],
        },
    },
}