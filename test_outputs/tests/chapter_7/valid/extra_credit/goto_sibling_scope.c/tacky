Program {
    static_variables: [],
    functions: [
        Function {
            identifier: "main",
            global: true,
            params: [],
            instructions: [
                Copy {
                    src: 0,
                    dst: Var("sum.1", Int),
                },
                JumpIfZero(
                    1,
                    "main.0.true",
                ),
                Copy {
                    src: 5,
                    dst: Var("a.2", Int),
                },
                Jump(
                    "main.other_if",
                ),
                Copy {
                    src: 0,
                    dst: Var("sum.1", Int),
                },
                Label(
                    "main.first_if",
                ),
                Copy {
                    src: 5,
                    dst: Var("a.2", Int),
                },
                Binary {
                    op: Add,
                    lhs: Var("sum.1", Int),
                    rhs: Var("a.2", Int),
                    dst: Var("main.tmp.0", Int),
                },
                Copy {
                    src: Var("main.tmp.0", Int),
                    dst: Var("sum.1", Int),
                },
                Jump(
                    "main.0.end",
                ),
                Label(
                    "main.0.true",
                ),
                Label(
                    "main.0.end",
                ),
                JumpIfZero(
                    0,
                    "main.1.true",
                ),
                Label(
                    "main.other_if",
                ),
                Copy {
                    src: 6,
                    dst: Var("a.3", Int),
                },
                Binary {
                    op: Add,
                    lhs: Var("sum.1", Int),
                    rhs: Var("a.3", Int),
                    dst: Var("main.tmp.1", Int),
                },
                Copy {
                    src: Var("main.tmp.1", Int),
                    dst: Var("sum.1", Int),
                },
                Jump(
                    "main.first_if",
                ),
                Copy {
                    src: 0,
                    dst: Var("sum.1", Int),
                },
                Jump(
                    "main.1.end",
                ),
                Label(
                    "main.1.true",
                ),
                Label(
                    "main.1.end",
                ),
                Return(
                    Var("sum.1", Int),
                ),
                Return(
                    0,
                ),
            ],
        },
    ],
}