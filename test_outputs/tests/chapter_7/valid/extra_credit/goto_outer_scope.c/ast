                If(
                    Var("a"),
                    Compound(
                        [
                            VariableDeclaration {
                                name: "a",
                                ty: Int,
                                init: Constant(1),
                            },
                            Expression(
                                Assignment {
                                    lhs: Var("b"),
                                    rhs: Var("a"),
                                },
                            ),
                            Goto("end"),
                        ],
                    ),
                    None,
                ),
                Expression(
                    Assignment {
                        lhs: Var("a"),
                        rhs: Constant(9),
                    },
                ),
                Labeled(
